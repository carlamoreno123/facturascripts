<?xml version="1.0" encoding="UTF-8"?>
<table>
    <column>
        <name>idmatricula</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>idalumno</name>
        <type>character varying(100)</type>
        <null>NO</null>
    </column>
     <column>
        <name>idasignatura</name>
        <type>character varying(9)</type>
    </column>
     <column>
        <name>fechaalta</name>
        <type>date</type>
        <null>NO</null>
    </column>
     <column>
        <name>fechabaja</name>
        <type>date</type>
        <null>NO</null>
    </column>
     <column>
        <name>cuota</name>
        <type>money</type>
        <null>NO</null>
    </column>
    <constraint>
        <name>matriculas_pkey</name>
        <type>PRIMARY KEY (idmatricula)</type>
        </constraint>
    <constraint>
        <name>ca_matriculas_alumno</name>
        <type>FOREIGN KEY (idalumno) REFERENCES alumnos (idalumno) ON DELETE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_matriculas_asignatura</name>
        <type>FOREIGN KEY (idasignatura) REFERENCES asignaturas (idasignatura) ON DELETE CASCADE</type>
    </constraint>
</table>